
public class Animal {
	
	Animal(Boolean isExtinct){
		Boolean extinct = isExtinct;
	}
	class mammal{
		mammal(String speciesType,int legsAmount,Boolean Flying){
			String species = speciesType;
			int legs = legsAmount;
			Boolean Flight = Flying;
		}
	}
	class reptile{
		reptile(String speciesType,int legsAmount,Boolean Flying){
			String species = speciesType;
			int legs = legsAmount;
			Boolean Flight = Flying;
		}
	}
	class bird{
		bird(String speciesType,int legsAmount,Boolean Flying){
			String species = speciesType;
			int legs = legsAmount;
			Boolean Flight = Flying;
		}
	}
	class fish{
		fish(String speciesType,int legsAmount,Boolean Flying){
			String species = speciesType;
			int legs = legsAmount;
			Boolean Flight = Flying;
		}
	}
	class insect{
		insect(String speciesType,int legsAmount,Boolean Flying){
			String species = speciesType;
			int legs = legsAmount;
			Boolean Flight = Flying;
		}
	}
	class micro{
		micro(String speciesType,int legsAmount,Boolean Flying){
			String species = speciesType;
			int legs = legsAmount;
			Boolean Flight = Flying;
		}
	}
}
